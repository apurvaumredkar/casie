# CASIE Bridge Environment Variables
# Copy this file to .env and fill in your actual values

# API Authentication (must match CASIE_BRIDGE_API_TOKEN in core/.env)
API_AUTH_TOKEN=generate_random_bearer_token

# ngrok Configuration
NGROK_AUTHTOKEN=your_ngrok_authtoken_from_dashboard
NGROK_DOMAIN=your-static-domain.ngrok-free.app

# Local Media Library
TV_DIRECTORY=C:\path\to\your\TV\shows\directory

# Cloudflare D1 Database
D1_DATABASE_ID=your_d1_database_id_from_wrangler
