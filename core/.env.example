# CASIE Worker Environment Variables
# Copy this file to .env and fill in your actual values

# Discord Configuration
DISCORD_PUBLIC_KEY=your_discord_public_key_here
DISCORD_BOT_TOKEN=your_discord_bot_token_here
APPLICATION_ID=your_discord_application_id_here
YOUR_DISCORD_ID=your_discord_user_id_for_pc_control

# AI/LLM Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Search API
BRAVE_API_KEY=your_brave_search_api_key_here

# Spotify OAuth
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
SPOTIFY_REDIRECT_URI=https://your-worker.workers.dev/callback
SPOTIFY_STATE_SECRET=generate_random_32_byte_secret

# CASIE Bridge Authentication
CASIE_BRIDGE_API_TOKEN=generate_random_bearer_token
